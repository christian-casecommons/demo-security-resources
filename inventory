# Each environment must be defined here
#
# Ensure each environment follows the example below and also create a file
# called group_vars/<environment>/vars.yml to hold environment specific settings

[non-prod]
non-prod ansible_connection=local

[demo-users]
demo-users ansible_connection=local ansible_python_interpreter=/usr/local/bin/python

[demo-resources]
demo-resources ansible_connection=local ansible_python_interpreter=/usr/local/bin/python